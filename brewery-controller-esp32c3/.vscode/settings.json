{
    "runInTerminal.commands": [
        {
            "match": "\\.rs$",
            "name": "run",
            "cmd": "pushd /home/anna/workspace/brewery-controller/brewery-controller-esp32c3 > /dev/null; cargo run; popd > /dev/null"
        }, 
        {
            "match": "\\.rs$",
            "name": "run-release",
            "cmd": "pushd /home/anna/workspace/brewery-controller/brewery-controller-esp32c3 > /dev/null; cargo run --release; popd > /dev/null"
        }, 
        {
            "match": "\\.rs$",
            "name": "stop",
            "cmd": "\u0003"
        },
    ],
    "rust-analyzer.checkOnSave.allTargets": false,
}
